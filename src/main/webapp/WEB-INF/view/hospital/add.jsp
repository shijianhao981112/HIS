<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">

<html>
<head>
    <title>录入住院信息-- -2015</title>
    <meta charset="UTF-8">
    <link rel="stylesheet" type="text/css" href="${pageContext.request.contextPath}/dist/Css/bootstrap.css" />
    <link rel="stylesheet" type="text/css" href="${pageContext.request.contextPath}/dist/Css/bootstrap-responsive.css" />
    <link rel="stylesheet" type="text/css" href="${pageContext.request.contextPath}/dist/Css/style.css" />
    <script type="text/javascript" src="${pageContext.request.contextPath}/dist/Js/jquery.js"></script>
    <script type="text/javascript" src="${pageContext.request.contextPath}/dist/Js/jquery.sorted.js"></script>
    <script type="text/javascript" src="${pageContext.request.contextPath}/dist/Js/bootstrap.js"></script>
    <script type="text/javascript" src="${pageContext.request.contextPath}/dist/Js/ckform.js"></script>
    <script type="text/javascript" src="${pageContext.request.contextPath}/dist/Js/common.js"></script>
    <script type="text/javascript" src="${pageContext.request.contextPath}/dist/Js/ckeditor/ckeditor.js"></script>
 

    <style type="text/css">
        body {
            padding-bottom: 40px;
        }
        .sidebar-nav {
            padding: 9px 0;
        }

        @media (max-width: 980px) {
            /* Enable use of floated navbar text */
            .navbar-text.pull-right {
                float: none;
                padding-left: 5px;
                padding-right: 5px;
            }
        }


    </style>
    <script type="text/javascript">
    $(function () {       
		$('#backid').click(function(){
				window.location.href="${pageContext.request.contextPath}/hospital/index";
		 });
    });
    </script>
    
   
</head>
<body>


<form action="${pageContext.request.contextPath}/hospital/add/look" method="post" class="definewidth m20">
<table class="table table-bordered table-hover definewidth m10">
    <tr>
        <td width="10%" class="tableleft">病历号</td>
        <td><input type="text" name="medical_record" id="b1" placeholder="输入病历号回车自动带出挂号人信息" value="${registration[0].medical_record  }"/>	
        	
        </td>
    	
    </tr>
    <tr>
        <td width="10%" class="tableleft">姓名</td>
        <td >
        	<input type="text" id="t1">
        ${registration[0].name }</td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">证件类型</td>
        <td>${registration[0].certificateType.name }</td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">证件号</td>
        <td>${registration[0].ID_num }</td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">社保号</td>
        <td>${registration[0].social_safe_num }</td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">联系电话</td>
        <td>${registration[0].phone }</td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">是否自费</td>
        <td>${registration[0].self_paying==1?'是':'否' }</td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">性别</td>
        <td>${registration[0].sex ==1?'男':'女'}</td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">年龄</td>
        <td>${registration[0].age }</td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">职业</td>
        <td>${registration[0].career }</td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">初复诊</td>
        <td>${registration[0].early_appointment }</td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">所挂科室</td>
        <td>${registration[0].section.secco_name }</td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">指定医生</td>
        <td>${registration[0].doctor.name }</td>
    </tr>
	<tr>
        <td width="10%" class="tableleft">备注</td>
        <td>${registration[0].noted }</td>
	</tr> 
</table>
</form>
<form action="${pageContext.request.contextPath}/hospital/add" method="post" class="definewidth m20">
<input type="hidden" name="medical_record" value="${registration[0].medical_record  }" />

<table class="table table-bordered table-hover definewidth m10">
    
    <tr>
        <td width="10%" class="tableleft">护理</td>
        <td><input type="text" name="caregiver" value=""/></td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">床位号</td>
        <td><input type="text" name="bed_id" value=""/></td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">缴费押金</td>
        <td><input type="text" name="pay_the_deposit" value=""/></td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">病情</td>
        <td><textarea name="state_illness"></textarea></td>
    </tr>
    <tr>
        <td colspan="2">
			<center>
				<button type="submit" class="btn btn-primary" type="button">保存</button> &nbsp;&nbsp;<button type="button" class="btn btn-success" name="backid" id="backid">返回列表</button>
			</center>
		</td>
    </tr>
</table>
</form>
                                                                                            
</body>
 <script type="text/javascript">
    	$("#b1").blur(function(){ // 失去焦点事件
    		var val =$("#b1").val(); // 获得输入的值
    		alert(val)
    		$.ajax({
    			url:"/A_Hospital/hospital/add/look",
    			type:"POST",
    			data:{"medical_record":val},
    			success:function(data){
    				alert( typeof date );
    				alert( "11"+data.object.registration );
    				var va =$("#t1").val(data.object.registration.name);
    			}
    		});
