<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
    <title>修改住院信息-- -2015</title>
    <meta charset="UTF-8">
    <link rel="stylesheet" type="text/css" href="${pageContext.request.contextPath}/dist/Css/bootstrap.css" />
    <link rel="stylesheet" type="text/css" href="${pageContext.request.contextPath}/dist/Css/bootstrap-responsive.css" />
    <link rel="stylesheet" type="text/css" href="${pageContext.request.contextPath}/dist/Css/style.css" />
    <script type="text/javascript" src="${pageContext.request.contextPath}/dist/Js/jquery.js"></script>
    <script type="text/javascript" src="${pageContext.request.contextPath}/dist/Js/jquery.sorted.js"></script>
    <script type="text/javascript" src="${pageContext.request.contextPath}/dist/Js/bootstrap.js"></script>
    <script type="text/javascript" src="${pageContext.request.contextPath}/dist/Js/ckform.js"></script>
    <script type="text/javascript" src="${pageContext.request.contextPath}/dist/Js/common.js"></script>
    <script type="text/javascript" src="${pageContext.request.contextPath}/dist/Js/ckeditor/ckeditor.js"></script>
 

    <style type="text/css">
        body {
            padding-bottom: 40px;
        }
        .sidebar-nav {
            padding: 9px 0;
        }

        @media (max-width: 980px) {
            /* Enable use of floated navbar text */
            .navbar-text.pull-right {
                float: none;
                padding-left: 5px;
                padding-right: 5px;
            }
        }


    </style>
    <script type="text/javascript">
    $(function () {       
		$('#backid').click(function(){
				window.location.href="${pageContext.request.contextPath}/hospital/index";
		 });
    });
    </script>
</head>
<body>
<form action="${pageContext.request.contextPath}/hospital/edit" method="post" class="definewidth m20">
<table class="table table-bordered table-hover definewidth m10">
    <tr>
        <td width="10%" class="tableleft">病历号</td>
        <td>${h.medical_record }
        <input type="hidden" name="medical_record" value="${h.medical_record }" >
        
        </td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">姓名</td>
        <td>${h.registrationInfor.name }</td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">证件类型</td>
        <td>${h.registrationInfor.certificateType.name }</td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">证件号</td>
        <td>${h.registrationInfor.ID_num }</td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">社保号</td>
        <td>${h.registrationInfor.social_safe_num }</td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">联系电话</td>
        <td>${h.registrationInfor.phone }</td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">是否自费</td>
        <td>
        <input type="radio" name="self_paying" value="0" checked/>否&nbsp;&nbsp;&nbsp;
        <input type="radio" name="self_paying" value="1"/>是
        </td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">性别</td>
        <td>
        <input type="radio" name="sex" value="1" checked/>男&nbsp;&nbsp;&nbsp;
        <input type="radio" name="sex" value="0"/>女
        </td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">年龄</td>
        <td>${h.registrationInfor.age }</td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">初复诊</td>
        <td><input type="radio" name="early_appointment" value="初诊" checked/>初诊&nbsp;&nbsp;&nbsp;
        <input type="radio" name="early_appointment" value="复诊"/>复诊
        </td>
    </tr>
    <tr>
    	<td width="10%" class="tableleft">所挂科室</td>
        <td>
        	<select name="section_id">
        	<option value="4">急诊科</option>
        	<option value="2">骨科</option>
        	<option value="1">血液科</option>
        	</select>
        </td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">指定医生</td>
		<td><select name="doctor_id">
	        <option value="1">华佗</option>
	        <option value="2">扁鹊</option>
	        <option value="3">李时珍</option>
       	 	</select>
        </td>
    </tr>
	<tr>
        <td width="10%" class="tableleft">备注</td>
       <td><textarea name="noted" >${h.registrationInfor.noted }</textarea></td>

	</tr>
    <tr>
        <td width="10%" class="tableleft">护理</td>
        <td><input type="text" name="caregiver" value="${h.caregiver }"/></td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">床位号</td>
        <td><input type="text" name="bed_id" value="${h.bed_id }"/></td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">缴费押金</td>
        <td><input type="text" name="pay_the_deposit" value="${h.pay_the_deposit }"/></td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">病情</td>
        <td><textarea name="state_illness">${h.state_illness }</textarea></td>
    </tr>
    <tr>
        <td colspan="2">
			<center>
				<button type="submit" class="btn btn-primary" type="button">保存</button> &nbsp;&nbsp;<button type="button" class="btn btn-success" name="backid" id="backid">返回列表</button>
			</center>
		</td>
    </tr>
</table>
