<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
    <title>修改药品---2015</title>
    <meta charset="UTF-8">
    <link rel="stylesheet" href="${pageContext.request.contextPath}/layui/css/layui.css">
    
    <link rel="stylesheet" type="text/css" href="${pageContext.request.contextPath}/dist/Css/bootstrap.css" />
    <link rel="stylesheet" type="text/css" href="${pageContext.request.contextPath}/dist/Css/bootstrap-responsive.css" />
    <link rel="stylesheet" type="text/css" href="${pageContext.request.contextPath}/dist/Css/style.css" />
    <script type="text/javascript" src="${pageContext.request.contextPath}/dist/Js/jquery.js"></script>
    <script type="text/javascript" src="${pageContext.request.contextPath}/dist/Js/jquery.sorted.js"></script>
    <script type="text/javascript" src="${pageContext.request.contextPath}/dist/Js/bootstrap.js"></script>
    <script type="text/javascript" src="${pageContext.request.contextPath}/dist/Js/ckform.js"></script>
    <script type="text/javascript" src="${pageContext.request.contextPath}/dist/Js/common.js"></script>
    <script type="text/javascript" src="${pageContext.request.contextPath}/dist/Js/ckeditor/ckeditor.js"></script>
 

    <style type="text/css">
        body {
            padding-bottom: 40px;
        }
        .sidebar-nav {
            padding: 9px 0;
        }

        @media (max-width: 980px) {
            /* Enable use of floated navbar text */
            .navbar-text.pull-right {
                float: none;
                padding-left: 5px;
                padding-right: 5px;
            }
        }


    </style>
    <script type="text/javascript">
    $(function () {       
		$('#backid').click(function(){
				window.location.href="${pageContext.request.contextPath}/medicine/index";
		 });
    });
    </script>
 
 
</head>
<body>
<form action="${pageContext.request.contextPath}/medicine/edit" method="post" class="definewidth m20">
<input type="hidden" name="drug_num" value="${medicine.drug_num }" >

<table class="table table-bordered table-hover definewidth m10">
    <tr>
        <td width="10%" class="tableleft">图片</td>
        <td><input type="file" name="drug_url" value="${medicine.drug_url }"/></td>
    </tr>
    
    <tr>
        <td width="10%" class="tableleft">进价</td>
        <td><input type="text" name="purchas_price" value="${medicine.purchas_price }"/></td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">售价</td>
        <td><input type="text" name="sale_price" value="${medicine.sale_price }"/></td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">药品名称</td>
        <td><input type="text" name="drug_name" value="${medicine.drug_name }"/></td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">药品类型</td>
        <td><input type="radio" name="drug_type" value="1" checked/>中药&nbsp;&nbsp;&nbsp;
        	<input type="radio" name="drug_type" value="2"/>西药&nbsp;&nbsp;&nbsp;
        	<input type="radio" name="drug_type" value="3"/>处方药&nbsp;&nbsp;&nbsp;
        	<input type="radio" name="drug_type" value="4"/>非处方药</td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">简单描述</td>
        <td><input type="text" name="simple_detail" value="${medicine.simple_detail }"/></td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">生产日期</td>
        <td><input type="text" name="prodeced_date" value="${medicine.prodeced_date }"/></td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">过期日期</td>
        <td><input type="text" name="expiration_date" value="${medicine.expiration_date }"/></td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">保质期</td>
        <td><input type="text" name="quality_date" value="${medicine.quality_date }"/>天</td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">详细描述</td>
        <td><textarea name="detail">${medicine.detail }</textarea></td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">生产厂商</td>
        <td><textarea name="manufacture">${medicine.manufacture }</textarea></td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">服用说明</td>
        <td><input type="text" name="use_infor" value="${medicine.use_infor }"/></td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">总的进货量</td>
        <td><input type="text" name="total_stock" value="${medicine.total_stock }"/></td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">剩余量</td>
        <td><input type="text" name="surplus" value="${medicine.surplus }"/></td>
        
    </tr>
	<tr>
        <td width="10%" class="tableleft">备注</td>
        <td><textarea name="noted">${medicine.noted }</textarea></td>
	</tr>
   
   
   
    <tr>
        <td colspan="2">
			<center>
				<button type="submit" class="btn btn-primary" type="button">保存</button> &nbsp;&nbsp;<button type="button" class="btn btn-success" name="backid" id="backid">返回列表</button>
			</center>
		</td>
    </tr>
</table>
